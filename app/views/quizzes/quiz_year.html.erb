<html>
  <head>
    <script type='text/javascript' src='https://ajax.googleapis.com/ajax/libs/jquery/1.7.0/jquery.min.js'></script>
    <script src="./assets/application.js"></script>
    <script src="./assets/flickQuiz_year.js"></script>
  </head>



  <body>
    <div id='main-container' class='container'>
      <div class="row">
        <div class='col-md-3'>
          </div>
            <div class="col-md-6 row text-center jumbotron">
                <h1>Guess The Year</h1>
                <h3>Film: <div id='title'></div></h3></br>
                <h3>Director: <div id='director'></div></h3></br>
                  <div id='poster'class='img-thumbnail' ></div>
                  <br>
                    <div class="form-group">
                  <br>                        
                  <h5>Which year: <br> <input id="yearGuess" type="text" name="yearGuess" class="form-control-static"></h5>
                    <br>
                    <input onclick="submitAnswer()"type="submit" class='btn btn-info'>
                    <input type="hidden" id="yearAnswer" name="yearAnswer" value="yearAnswer"/>
                      <div id='result'></div>
                      <br>
                      <div id='answer'></div>
                      <br>
                    <input id='reset' type='button' value='Play Again' onClick='newFilm()'class='btn btn-info'>
                    </div>
            </div>
          <div class="col-md-3"></div>
      </div>
    </div>
  </body>
</html>